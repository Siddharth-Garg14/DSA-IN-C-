#include <bits/stdc++.h>
using namespace std;

bool check(vector<int> arr)
{
    int low =arr[0];
    for(int i=0;i<arr.size()-1;i++)
    {
        if ( arr[i] > arr[i+1] )
            return false;
    }
    return true;
}
int main()
{
    vector<int> arr={5,4,6,7,8};
    bool c=check(arr);
    cout<<c;
    
}
